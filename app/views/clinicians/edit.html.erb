<%= form_for @clinician do |form| %>
  <div class="form-group">
    <%= form.label :first_name %>
    <%= form.text_field :first_name, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :last_name %>
  	<%= form.text_field :last_name, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :occupation %>
  	<%= form.text_field :occupation, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :gender %>
  	<%= form.text_field :gender, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :email %>
  	<%= form.text_field :email, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :password %>
  	<%= form.text_field :password, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :office_number %>
  	<%= form.text_field :office_number, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :mobile_number %>
  	<%= form.text_field :mobile_number, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :emergency_message %>
  	<%= form.text_field :emergency_message, class: "form-control" %>
  </div>

  <div class="form-group">
  	<%= form.label :care_group_id %>
  	<%= form.text_field :care_group_id, class: "form-control" %>
  </div>
  <%= form.button 'Update Clinician', class: "btn btn-success" %>
<% end %>