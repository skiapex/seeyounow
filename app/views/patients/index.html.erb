<h3 class="page-header">
  All Actors
  <span class="pull-right">
    <%= link_to "New Patient", new_patient_path, class: 'btn btn-sm btn-success' %>
  </span>
</h3>

<ul>
  <% @patients.each do |patient| %>
    <li><%= patient.first_name patient.last_name %> <%= link_to "Edit", edit_patient_path(patient) %> <%= link_to "Delete", patient_path(patient), method: :delete, data: {confirm: 'Are you sure you want to delete'} %></li>
  <% end %>
</ul>