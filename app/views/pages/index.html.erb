<h3 class="page-header">
  All Clinicians
  <span class="pull-right">
    <%= link_to "New Clinician", new_clinician_path, class: 'btn btn-sm btn-success' %>
  </span>
</h3>

<ul>
  <% @clinicians.each do |clinician| %>
    <li><h3><%= link_to "#{clinician.first_name} #{clinician.last_name}", clinician_path(clinician) %></h3><h4><%= clinician.occupation.name %></h4> <%= link_to "Edit", edit_clinician_path(clinician) %> <%= link_to "Delete", clinician_path(clinician), method: :delete, data: {confirm: 'Are you sure you want to delete'} %></li>
  <% end %>
</ul>